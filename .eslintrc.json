{
  "env": {
    "browser": true,
    "es2021": true,
     "commonjs": true,
     "es6": true,
    "node": true

  },
  "extends": [
    "react-app",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  }
}